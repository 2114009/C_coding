#include <stdio.h>

int main() {
	int a=10,b=5,c=15;
	printf("Enter the numbers:  %d, %d, %d \n", a, b, c);
	
	if (a>=b)
	{
	    if (a>=c)
	    {
	        printf("%d is the largest",a);
	    }
	    else
	    {
	        printf("%d is the largest",c);
	    }
	}
	else
	{
	    if (b>=c)
	    {
	        printf("%d is the largest",b);
	    }
	    else
	    {
	        printf("%d is the largest",c);
	    }
	}

	return 0;
}
